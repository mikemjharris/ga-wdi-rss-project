<h1>User Homepage</h1>

<h2>Subscriptions</h2>
<% @user.feeds.each do | feed | %>
	<p><%= feed.title %></p>
<% end %>

<h2>Articles</h2>
<% @user.feeds.each do | feed | %>
	<% feed.articles.each do | article | %>
	<h3><%= article.title %></h3>
	<p><%= article.summary %></p>
	<a href="<%= article.url %>">Read More...</a>
	<% end %>
<% end %>
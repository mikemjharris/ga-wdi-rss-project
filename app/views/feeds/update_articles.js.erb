
var list_of_feeds = $('.feed_title')
var last_updated = $('.feed_title').data('last_updated')

var articleHtml =  "<%= escape_javascript(render partial: '/users/articles' ) %>"

$('#articles').html(articleHtml)

$('.article').each(function(i,el){

  if($(el).data('last_updated') > last_updated){
    
    
    setTimeout(function () { 
        $(el).addClass('article_flash');
      }, 100);
    setTimeout(function () { 
        $(el).removeClass('article_flash');
      }, 1000);
    }
   
});

getReady()

